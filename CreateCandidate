response = mergeEventObj.get("response");
docInfo = response.get("mergedDocInfo");
getID = zoho.people.getRecords("Stryve_New_Hire_Form",0);
getCandZero = getID.get(0);
getCand = getCandZero.get("New_Hire_ID");
candFirstName = getCandZero.get("First_Name");
candLastName = getCandZero.get("Family_Name");
candEmail = getCandZero.get("Email_Address");
candPhone = getCandZero.get("Mobile");
candDOB = getCandZero.get("DOB");
candNat = getCandZero.get("Nationality");
candStart = getCandZero.get("Start_Date1");
candAdd1 = getCandZero.get("Address_Line_1");
candAdd2 = getCandZero.get("Address_Line_2");
candCity = getCandZero.get("City");
candState = getCandZero.get("State");
candCountry = getCandZero.get("Country1");
candPost = getCandZero.get("Postal_Code");
fieldMap = Map();
fieldMap.put("First_Name",candFirstName);
fieldMap.put("Last_Name",candLastName);
fieldMap.put("Email_ID",candEmail);
fieldMap.put("Mobile",candPhone);
fieldMap.put("Date_of_Birth",candDOB);
fieldMap.put("Other_Email",candEmail);
fieldMap.put("Nationality",candNat);
fieldMap.put("Start_Date",candStart);
fieldMap.put("Address_Line_1",candAdd1);
fieldMap.put("Address_Line_2",candAdd2);
fieldMap.put("City",candCity);
fieldMap.put("State",candState);
fieldMap.put("Country",candCountry);
fieldMap.put("Postal_Code",candPost);
newCand = zoho.people.create("Candidate",fieldMap);
CandID = newCand.get("result").get("pkId");
info CandID;
